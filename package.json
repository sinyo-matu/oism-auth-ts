{
  "name": "oism-auth",
  "version": "0.3.4",
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./index.d.ts",
      "module": "./index.js",
      "import": "./index.js",
      "require": "./index.cjs",
      "default": "./index.js"
    },
    "./*": {
      "types": "./*.d.ts",
      "module": "./*.js",
      "import": "./*.js",
      "require": "./*.cjs",
      "default": "./*.js"
    }
  },
  "description": "OISM Authentication",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup --entry src/index.ts --entry src/svelte-kit.ts --entry src/nextjs.ts --format cjs,esm --dts --outDir ./",
    "build:dev": "tsup --entry src/index.ts --entry src/svelte-kit.ts --entry src/nextjs.ts --format cjs,esm --dts",
    "lint": "tsc",
    "publish": "pnpm run build&&pnpm changeset publish"
  },
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@sveltejs/kit": "^1.2.3",
    "next": "^13.1.4",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "vitest": "^0.27.3"
  },
  "dependencies": {
    "nookies": "^2.5.2",
    "zod": "^3.20.2"
  }
}
